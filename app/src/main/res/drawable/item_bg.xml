<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:color="#40b2cf" android:width="1dp" />
            <solid android:color="@color/colorCompletePressed"></solid>
            <!--<size android:width="150dp" android:height="150dp"/>-->
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:color="#4bcff1" android:width="1dp" />
            <solid android:color="@color/colorComplete"/>
        </shape>
    </item>
</selector>