<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:color="#40b2cf" android:width="1dp" />
            <solid android:color="#bcad42"></solid>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:color="#40b2cf" android:width="1dp" />
            <solid android:color="@color/colorCompleteDisable"/>
        </shape>
    </item>
</selector>